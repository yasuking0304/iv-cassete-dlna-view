<Application x:Class="View.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml" Startup="OnStartup" Exit="OnExit">
    <Application.Resources>
        <!-- アプリケーション レベルのスコープにあるリソースはここで定義します。-->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source=".\Resources\String.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <!-- アプリケーション用フォントファミリリソース -->
            <FontFamily x:Key="ApplicationFontFamily">MS UI Gothic</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
